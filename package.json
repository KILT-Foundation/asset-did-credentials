{
  "name": "asset-did-credentials",
  "version": "1.0.0",
  "repository": "git@github.com:KILT-Foundation/asset-did-credentials.git",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=v20.0"
  },
  "scripts": {
    "dev": "parcel --open",
    "build": "parcel build",
    "test": "tsc --noEmit",
    "lint": "yarn run lint-css && yarn run lint-js",
    "lint-js": "eslint --max-warnings 0 --config ./.eslintrc.json \"src/**/*.ts*\"",
    "lint-css": "stylelint \"src/**/*.css\"",
    "fix-css": "stylelint --fix \"src/**/*.css\"",
    "css": "tcm --pattern 'src/**/*.module.css' && eslint --fix --config ./.eslintrc.json \"src/**/*.css.d.ts\"",
    "prettify": "prettier --write \"src/**/*.ts*\""
  },
  "dependencies": {
    "@kiltprotocol/asset-did": "^0.32.0",
    "@kiltprotocol/sdk-js": "^0.32.0",
    "@polkadot/extension-dapp": "^0.44.8",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.8",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "buffer": "^6.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "events": "^3.3.0",
    "parcel": "^2.12.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^29.0.0",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "resolutions": {
    "terser": "5.16.1"
  },
  "targets": {
    "default": {
      "source": "src/index.html",
      "distDir": "dist",
      "engines": {
        "browsers": "last 2 Chrome versions, last 2 Firefox versions"
      }
    }
  }
}
